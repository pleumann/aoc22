; Exit:     -
; Uses:     HL,DE
;
__length:
        ld      hl,2
        add     hl,sp
        ld      d,0
        ld      e,(hl)
        inc     h
        ld      (hl),de
        ret

;
; Load string from HL to stack, return properly.
;
__loadstr:
        pop     bc
        ex      de,hl
        ld      hl,-256
        add     hl,sp
        ld      sp,hl
        ex      de,hl
        push    bc
        ld      a,255
        call    __movestr
        ret

;
; Store string of max length A from stack to address, cleanup stack, return properly.
;
__storestr:
        ld      hl,258
        add     hl,sp
        ld      de,(hl)
        dec     h
        call    __movestr
        pop     bc
        ld      hl,258
        add     hl,sp
        ld      sp,hl
        push    bc
        ret

;
; Move string of max length A from HL to DE, bypassing the stack.
;
__movestr:
        ld      b,(hl)
        cp      b
        jp      c,__movestr_1
        ld      a,b
__movestr_1:
        ld      (de),a
        and     a
        ret     z
        inc     hl
        inc     de
        ld      b,0
        ld      c,a
        ldir
        ret

